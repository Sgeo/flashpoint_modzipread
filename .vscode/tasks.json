{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/LD",
                "/EHsc",
                "/Fe:",
                "${fileDirname}\\${fileBasenameNoExtension}.so",
                "${file}",
                "vcpkg\\installed\\x86-windows-static\\lib\\*.lib",
                "apache\\lib\\*.lib",
                "/Iapache\\include",
                "/Ivcpkg\\installed\\x86-windows-static\\include\\",
                "/link /MACHINE:X86 /export:zipread_module,DATA"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}